{
    "tags": {
        "allowUnknownTags": false
    },
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
        "template": "docs/themes/docdash/",
        "encoding": "utf8",
        "destination": "docs/dist/js",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "tutorials": "docs/assets/jsdocs/"
}